<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data noupdate="1">
    <record id="document_sequence" model="ir.sequence">
      <field name="name">文件管理序列</field>
      <!-- 唯一標識符，用于代碼中調用 -->
      <field name="code">document_sequence</field>
      <!-- 前綴固定爲 -->
      <field name="prefix">DOC-%(year)s-</field>
      <!-- 序列號位數（6位，即000001） -->
      <field name="padding">6</field>
      <!-- 跨公司通用 -->
      <field name="company_id" eval="False" />
      <!-- standard允許斷號，no_gap確保連續（性能開銷較大） -->
      <field name="implementation">standard</field>
    </record>
