<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- RFQ表單狀態 -->
        <record id="rfq_property_state_Preliminary" model="rfq.property.state">
            <field name="name">Preliminary</field>
            <field name="description">新建立</field>
            <field name="sequence" eval="1"/>
            <field name="folded" eval="False"/>
        </record>
        <record id="rfq_property_state_In_Review" model="rfq.property.state">
            <field name="name">In Review</field>
            <field name="description">審核中</field>
            <field name="sequence" eval="2"/>
            <field name="folded" eval="False"/>
        </record>
        <record id="rfq_property_state_On_Close" model="rfq.property.state">
            <field name="name">On Close</field>
            <field name="description">已結案</field>
            <field name="sequence" eval="3"/>
            <field name="folded" eval="True"/>
        </record>
        <!-- RFQ送審單狀態 -->
        <record id="rfq_flow_state_Preliminary" model="rfq.flow.state">
            <field name="name">Preliminary</field>
            <field name="description">新建立</field>
            <field name="sequence" eval="1"/>
            <field name="folded" eval="False"/>
        </record>
        <record id="rfq_flow_state_In_Review" model="rfq.flow.state">
            <field name="name">In Review</field>
            <field name="description">審核中</field>
            <field name="sequence" eval="2"/>
            <field name="folded" eval="False"/>
        </record>
        <record id="rfq_flow_state_On_Close" model="rfq.flow.state">
            <field name="name">On Close</field>
            <field name="description">已結案</field>
            <field name="sequence" eval="3"/>
            <field name="folded" eval="True"/>
        </record>
    </data>
</odoo>